{
  "name": "artivisi-website",
  "version": "1.0.0",
  "description": "ArtiVisi Intermedia Corporate Website",
  "scripts": {
    "build:css": "tailwindcss -i ./assets/css/tailwind.css -o ./static/css/main.css --minify",
    "dev:css": "tailwindcss -i ./assets/css/tailwind.css -o ./static/css/main.css --watch",
    "build": "npm run build:css && hugo --gc --minify",
    "dev": "npm run dev:css & hugo server -D"
  },
  "keywords": [
    "hugo",
    "website",
    "tailwindcss"
  ],
  "author": "ArtiVisi Intermedia",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "tailwindcss": "^4.1.17"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "jquery.magnify": "^1.6.3",
    "playwright": "^1.56.1",
    "swiper": "^11.0.6"
  }
}
